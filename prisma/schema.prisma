datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  email     String   @unique
  password  String
  name      String
  phone     String?
  active    Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

